{
  "name": "@duro/swc-plugin",
  "version": "0.0.0",
  "description": "SWC/Babel transform plugin for Duro sx prop",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "test": "vitest",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/types": "^7.23.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/traverse": "^7.23.0",
    "@duro/tsconfig": "workspace:*",
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/node": "^18.0.0",
    "eslint": "^8.15.0",
    "eslint-config-duro": "workspace:*",
    "tsup": "^6.0.0",
    "typescript": "^4.5.3",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
